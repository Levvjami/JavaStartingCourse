public class Main {
    public static void main (String[] args){
        System.out.println(toMilesPerHour(1.5));
    }

    public static long toMilesPerHour(double kilometersPerHour){
        if (kilometersPerHour < 0){
            return (long) -1.0;
        }
        return Math.round(kilometersPerHour * 0.601371192);
    }
}